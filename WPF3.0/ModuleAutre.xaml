<Page x:Class="WPF3._0.ModuleAutre"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF3._0"
      mc:Ignorable="d" 
      d:DesignHeight="375" d:DesignWidth="800"
      Title="ModuleAutre">

    <Grid Background="White">

        <Grid>
            <Grid.ColumnDefinitions />
            <StackPanel Orientation="Horizontal">

            </StackPanel>
        </Grid>

        <StackPanel Margin="40,40,0,0">
            <Button HorizontalAlignment="Left" Content="Client aléatoire" Width="150" Margin="0,0,0,40" Click="AfficheClientHasard"/>
            <Button HorizontalAlignment="Left" Content="Meilleur client" Width="150" Margin="0,0,0,40" Click="AfficheMeilleurClient"/>
            <Button HorizontalAlignment="Left" Content="Nom du jour" Width="150" Margin="0,0,0,40" Click="NomDuJour"/>
            <Button HorizontalAlignment="Left" Content="Nom de Zeus !" Width="150" Margin="0,0,0,40" Click="BackToTheFuture"/>

        </StackPanel>

        <Frame Grid.Row="1" Name="CommandeMain" />
    </Grid>
</Page>
